<!-- For kpi -> K*gamma in 840  -->

<?xml version="1.0"?>
<doc>
  <config>
    <Nt_corr>7</Nt_corr>
    <t_origin>0</t_origin>
    <bc_spec>-1</bc_spec>
    <convertUDtoL>true</convertUDtoL>
    <convertUDtoS>false</convertUDtoS>
    <average_1pt_diagrams>true</average_1pt_diagrams>
    <zeroUnsmearedGraphsP>true</zeroUnsmearedGraphsP>
    <ensemble>test</ensemble>
    <decayDir>3</decayDir>
    <lattSize>16 16 16 128</lattSize>
  </config>

  <Npts>3</Npts>

  <fl>
    <twoI>2 0 2</twoI>
    <threeY>3 0 3</threeY>
    <twoI_z>1 0 1</twoI_z>
  </fl>

  <creation_op>1 1 0</creation_op>
  <smearedP>1 0 1</smearedP>
  <t_slice>6 -3 0</t_slice>

  <temp_curr>true</temp_curr>
  <spa_curr>true</spa_curr>
  <twoJ>0 1</twoJ>

  <DBFiles>
    <proj_op_xmls>
      <elem>weights.pion.xml</elem>
      <elem>weights.kaon.xml</elem>
      <elem>weights.kbar.xml</elem>
      <elem>weights.rho.xml</elem>
    </proj_op_xmls>
    <corr_graph_db>eval_graph.sdb1</corr_graph_db>
    <noneval_graph_xml>noneval_graph.xml</noneval_graph_xml>
    <smeared_hadron_node_xml>smeared_hadron_node.xml</smeared_hadron_node_xml>
    <unsmeared_hadron_node_xml>unsmeared_hadron_node.xml</unsmeared_hadron_node_xml>
    <hadron_npt_graph_db>graph.sdb1</hadron_npt_graph_db>
    <hadron_node_dbs>
      <elem>smeared_hadron_node.sdb1</elem>
      <elem>unsmeared_hadron_node.sdb1</elem>
    </hadron_node_dbs>
    <output_db>output.npt.sdb1</output_db>
  </DBFiles>




</doc>
